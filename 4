#include <iostream>
using namespace std;
int ageTransform(int);
int main()
{
	int age,months;
	cout << "Enter your age:  岁\b\b\b\b";
	cin >> age;
	cin.get();
	months = ageTransform(age);
	cout <<"The month is "<< months << " 月";
	cin.get();
	return 0;
}
 int ageTransform(int age)
{
	int months = age*12;
	return months;
}
